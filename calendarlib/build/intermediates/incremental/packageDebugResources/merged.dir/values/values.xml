    <string name="app_name">FliexibleCalendar</string>
    <declare-styleable name="CellState">
        <attr format="boolean" name="state_date_regular"/>
        <attr format="boolean" name="state_date_today"/>
        <attr format="boolean" name="state_date_selected"/>
        <attr format="boolean" name="state_date_outside_month"/>
    </declare-styleable>
    <declare-styleable name="CircularEventCellView">
        <attr format="dimension" name="event_radius"/>
        <attr format="dimension" name="event_circle_padding"/>
    </declare-styleable>
    <declare-styleable name="EventCountCellView">
        <attr format="dimension" name="event_count_radius"/>
        <attr format="color" name="event_background"/>
        <attr format="color" name="event_count_text_color"/>
        <attr format="dimension" name="event_text_size"/>
    </declare-styleable>
    <declare-styleable name="FlexibleCalendarView">
        <attr format="integer" name="startDisplayYear"/>
        <attr format="integer" name="startDisplayMonth"/>
        <attr format="dimension" name="weekDayHorizontalSpacing"/>
        <attr format="dimension" name="weekDayVerticalSpacing"/>
        <attr format="dimension" name="monthDayHorizontalSpacing"/>
        <attr format="dimension" name="monthDayVerticalSpacing"/>
        <attr format="reference" name="weekViewBackground"/>
        <attr format="reference" name="monthViewBackground"/>
        <attr format="boolean" name="showDatesOutsideMonth"/>
        <attr format="boolean" name="decorateDatesOutsideMonth"/>
        <attr format="boolean" name="disableAutoDateSelection"/>
        <attr format="enum" name="startDayOfTheWeek">
            <enum name="SUNDAY" value="1"/>
            <enum name="MONDAY" value="2"/>
            <enum name="TUESDAY" value="3"/>
            <enum name="WEDNESDAY" value="4"/>
            <enum name="THURSDAY" value="5"/>
            <enum name="FRIDAY" value="6"/>
            <enum name="SATURDAY" value="7"/>
        </attr>
    </declare-styleable>
